{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/skarampatakis/easyrdf"
        },
        {
            "type": "git",
            "url": "https://github.com/rbruhn/stapler.git"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/framework": "5.6.*",
        "acacha/admin-lte-template-laravel": "^6.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "barryvdh/laravel-debugbar": "^3.1",
        "laravelcollective/html": "5.6.*",
        "codesleeve/laravel-stapler": "1.0.*",
        "codesleeve/stapler": "dev-master as 1.2.0",
        "easyrdf/easyrdf": "dev-okgreece",
        "ml/json-ld": "^1.0",
        "mbostock/d3": "v3.5.16",
        "doctrine/dbal": "v2.5.4",        
        "laravel/socialite": "^3.0",
        "vluzrmos/language-detector": "^1.0",
        "rairlie/laravel-locking-session": "^1.0",
        "yajra/laravel-datatables-oracle": "^8.0",
        "sabre/xml": "^2.0"        
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0",
        "symfony/css-selector": "2.8.*|3.1.*",
        "symfony/dom-crawler": "2.8.*|3.1.*",
        "filp/whoops": "~2.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "php artisan clear-compiled"            
        ],
        "pre-update-cmd": [
            
        ],
        "post-update-cmd": [
            
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
